<% include ../partials/header %>


<div class="container">
     <header class="jumbotron">
         <div class="container">
             <h1>Wood carving page</h1>
         </div>
     </header>
     
     <div class="row text-center" style="display:flex; flex-wrap: wrap;">
        <% carving.forEach(function(carvings){ %>
            <div class="col-md-3 col-sm-6">
                <div class="thumbnail">
                   <img src="<%= carving.image %>">
                   <div class="caption">
                       <h4><%= carving.name %></h4>
                   </div>
                </div>
            </div>
        <% }); %>
    </div>
</div>

<% include ../partials/footer %>